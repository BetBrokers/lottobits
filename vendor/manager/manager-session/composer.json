{
	"name" : "Manager/Session",
	"description" : "Manager file package",
	"license" : "BSD-3-Clause",
	"keywords" : [
		"manager"
	],
	"require" : {
		"php" : "^5.6 || ^7.0"
	},
	
	"config" : {
		"sort-packages" : true
	},

    "autoload": {
        "psr-4": {
            "CrowdForex\\File\\": "src/"
        }
    },
	"extra" : {
		"branch-alias" : {
			"dev-master" : "3.2.x-dev",
			"dev-develop" : "3.3.x-dev"
		}
	},
	
	"scripts" : {
		"check" : [
			"@cs-check",
			"@test"
		],
		"cs-check" : "phpcs",
		"cs-fix" : "phpcbf",
		"test" : "phpunit --colors=always",
		"test-coverage" : "phpunit --colors=always --coverage-clover clover.xml"
	}
}
